<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AlignTextView_Default">
        <item name="align">center</item>
    </style>

    <declare-styleable name="AlignTextView">
        <attr name="align">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>
</resources>