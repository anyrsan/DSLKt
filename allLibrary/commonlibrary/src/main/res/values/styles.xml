<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="DefaultSection" parent="AppTheme">
        <item name="cpSectionBackground">@color/color_ffffff</item>
        <item name="cpSectionTextColor">@color/color_e50000</item>
        <item name="cpSectionTextSize">@dimen/ts_18</item>
        <item name="cpSectionHeight">@dimen/dp_40</item>
    </style>
</resources>
